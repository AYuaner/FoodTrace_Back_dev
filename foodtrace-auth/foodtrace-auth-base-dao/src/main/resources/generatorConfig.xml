<!DOCTYPE generatorConfiguration PUBLIC
        "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <context id="mysql" targetRuntime="MyBatis3DynamicSql">
        <plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin"/>
        <plugin type="org.mybatis.generator.plugins.SerializablePlugin"/>

        <commentGenerator>
            <property name="suppressAllComments" value="false"/>
            <property name="addRemarkComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/foodtrace?useUnicode=true;characterEncoding=utf-8"
                        userId="root" password="ayuan520.">
            <property name="nullCatalogMeansCurrent" value="true"/>
        </jdbcConnection>

        <!-- dao entity设置 -->
        <javaModelGenerator targetPackage="com.yuan.foodtrace.auth.base.dao.entity" targetProject="src/main/java"/>
        <javaClientGenerator targetPackage="com.yuan.foodtrace.auth.base.dao.dao" targetProject="src/main/java"/>

        <!-- 用户管理 -->
        <table tableName="user" domainObjectName="UserRecord" mapperName="UserBaseMapper"/>

    </context>
</generatorConfiguration>
